% Read in the data from dataset.nt and then load the predicates 

:- reconsult('utils.P').

main :- 
    see('dataset.nt'),
    read_str(A),
    tokenize(A, List),
    bind_rdf(List, S, O, P),
    write(S),nl,
    write(O),nl,
    write(P),nl,nl,
    
    read_str(B),
    write(B), nl,
    seen.



