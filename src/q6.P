

result1(Product,Label,Type) :- 
	rdf3(Product, '<http://www.w3.org/2000/01/rdf-schema#label>', Label),
	rdf3(Product, '<http://www.w3.org/1999/02/22-rdf-syntax-ns#type>', Type).


query(FinalBag,Product,Label,Type):-
	bagof(query_intermediate(Product,Label,Type), query_intermediate(Product,Label,Type), ResultIntermediate),
	FinalBag = ResultIntermediate.

query_intermediate(Product,Label,Type):- result1(Product,Label,Type).
